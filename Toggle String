#include <iostream>
#include<string.h>
using namespace std;

int main() {

string s;
cin >> s;
int l  = s.length();
int i = 0;
while(i<l){
    if(s[i]>='a' && s[i]<='z')
    {
         s[i]=s[i]-32;
    }
    else if(s[i]>='A' && s[i]<='Z')
    {

        s[i]=s[i]+32;
    }

    i++;

}

cout<<s<<endl;

}
